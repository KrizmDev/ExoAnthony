@model IEnumerable<Exercice4.Models.Product>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Exercice4</title>
</head>
<body>
     @foreach(Product pr in Models){
       <ul>
        <li>@pr.Id</li>
        <li>@pr.Name</li>
        <li>@pr.Category</li>
        <li>@pr.Price</li>
       
       
       
       </ul>
     }
</body>
</html>
